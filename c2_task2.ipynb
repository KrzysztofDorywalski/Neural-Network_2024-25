    "x=torch.tensor(10., requires_grad=True)\n",
    "\n",
    "y=x**2\n",
    "y.backward()\n",
    "z=x**3\n",
    "r=(y+z).sum()\n",
    "#make_dot(r).render(\"attached\", format=\"png\")\n",
    "make_dot(r)"
   ]
